<Window x:Class="POP_SF_63_2017_GUI.GUI.NamestajWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF_63_2017_GUI.GUI"
        mc:Ignorable="d"
        Title="NamestajWindow" Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="37*"/>
            <ColumnDefinition Width="109*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" RenderTransformOrigin="0.497,0.53">Naziv:</Label>
        <TextBox Grid.Row="0" Height="30" HorizontalAlignment="left" VerticalAlignment="Top" Width="200" Margin="2,10,0,0" Name="tbNaziv" Grid.Column="1"/>
        <Button Grid.Row="1" HorizontalAlignment="Right" Name="btnSacuvaj" Click="btnSacuvaj_Click" Margin="0,87,113,10" Width="95" Grid.Column="1">Sacuvaj</Button>
        <Button Grid.Row="1" HorizontalAlignment="Right" Name="btnIzlaz" Click="btnIzlaz_Click" Margin="0,87,10,10" Width="95" Grid.Column="1">Zatvori</Button>
    </Grid>
</Window>
